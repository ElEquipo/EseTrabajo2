<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="ac_Empleado" fx:id="ac_empleado" prefHeight="576.0" prefWidth="1024.0" stylesheets="@empleado.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vista.Empleado.EmpleadoController">
   <children>
      <Pane id="pn_FondoEmpleado" layoutX="148.0" prefHeight="578.0" prefWidth="740.0" stylesheets="@empleado.css">
         <children>
            <Pane id="pn_CabeceraEmpleado" fx:id="pn_titulo" layoutX="-21.0" layoutY="30.0" prefHeight="93.0" prefWidth="790.0" stylesheets="@empleado.css">
               <children>
                  <Button id="bt_Inicio" fx:id="bt_Inicio" layoutX="27.0" mnemonicParsing="false" onAction="#inicioAction" prefHeight="72.0" prefWidth="141.0" stylesheets="@empleado.css">
                      <font>
                        <Font name="Arial" size="21.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="bt_Perfil" layoutX="475.0" mnemonicParsing="false" prefHeight="72.0" prefWidth="100.0" stylesheets="@empleado.css" text="Perfil" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button fx:id="bt_Ayuda" layoutX="573.0" mnemonicParsing="false" prefHeight="72.0" prefWidth="100.0" stylesheets="@empleado.css" text="Ayuda" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button id="bt_LogOut" fx:id="bt_LogOut" layoutX="673.0" mnemonicParsing="false" onAction="#CloseAction" prefHeight="72.0" prefWidth="90.0" stylesheets="@empleado.css">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
               </children>
            </Pane>
            <Pane id="pn_FondoIconos" fx:id="pn_fondoIconos" layoutY="114.0" prefHeight="464.0" prefWidth="744.0" stylesheets="@empleado.css">
               <children>
                  <Button fx:id="bt_ventas" layoutX="488.0" layoutY="177.0" mnemonicParsing="false" onAction="#ventasAction" prefHeight="110.0" prefWidth="111.0" stylesheets="@empleado.css" />
                  <Button fx:id="bt_productos" layoutX="138.0" layoutY="177.0" mnemonicParsing="false" onAction="#productosAction" prefHeight="110.0" prefWidth="111.0" />
               </children>
            </Pane>
            <Pane fx:id="pn_productos" layoutY="114.0" prefHeight="464.0" prefWidth="744.0">
               <children>
                  <TableView fx:id="tv_productos" layoutX="9.0" layoutY="28.0" prefHeight="291.0" prefWidth="719.0">
                     <columns>
                        <TableColumn fx:id="tb_referencia" prefWidth="64.0" text="referencia" />
                        <TableColumn fx:id="tb_nombre" prefWidth="82.0" text="nombre" />
                        <TableColumn fx:id="tb_categoria" prefWidth="121.0" text="categoría" />
                        <TableColumn fx:id="tb_descripcion" prefWidth="161.0" text="descripción" />
                        <TableColumn fx:id="tb_precioCompra" prefWidth="98.0" text="precio compra" />
                        <TableColumn fx:id="tb_precioVenta" prefWidth="113.0" text="precio venta" />
                        <TableColumn fx:id="tb_iva" prefWidth="77.0" text="IVA" />
                     </columns>
                  </TableView>
                  <Button id="bt_atras" fx:id="bt_atrasProductos" layoutX="316.0" layoutY="339.0" mnemonicParsing="false" onAction="#productosAction" prefHeight="111.0" prefWidth="112.0" stylesheets="@../gerente/gerente.css" />
               </children>
            </Pane>
            <Pane fx:id="pn_ventas" layoutY="114.0" prefHeight="464.0" prefWidth="744.0">
               <children>
                  <Label fx:id="lb_idVenta" layoutX="45.0" layoutY="75.0" prefHeight="25.0" prefWidth="66.0" />
                  <ComboBox fx:id="cb_tiendas" layoutX="45.0" layoutY="125.0" prefWidth="150.0" promptText="Tienda" />
                  <ComboBox fx:id="cb_trabajadores" layoutX="214.0" layoutY="125.0" prefWidth="150.0" promptText="Trabajador" />
                  <TextField fx:id="tf_fechaVenta" layoutX="386.0" layoutY="125.0" prefHeight="25.0" prefWidth="129.0" promptText="fecha venta" />
                  <Button id="bt_ventas" fx:id="bt_regVenta" layoutX="234.0" layoutY="253.0" mnemonicParsing="false" onAction="#ventasAction" prefHeight="110.0" prefWidth="111.0" stylesheets="@empleado.css" />
                  <Button id="bt_atras" fx:id="bt_atrasVentas" layoutX="604.0" layoutY="339.0" mnemonicParsing="false" onAction="#ventasAction" prefHeight="111.0" prefWidth="112.0" stylesheets="@../gerente/gerente.css" />
                  <ComboBox fx:id="cb_referencia" layoutX="45.0" layoutY="192.0" prefWidth="150.0" promptText="Referencia" />
                  <TextField fx:id="tf_cantidad" layoutX="215.0" layoutY="192.0" prefHeight="25.0" prefWidth="90.0" promptText="cantidad" />
               </children>
            </Pane>
         </children>
      </Pane>
   </children>
</AnchorPane>
